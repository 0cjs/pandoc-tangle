#!/usr/bin/env zsh

pandoc-tangle --from=markdown --code=example --to=markdown     Tangle.md > README.md
pandoc-tangle --from=markdown --code=main    --to=code-haskell Tangle.md > src/Main.hs
pandoc-tangle --from=markdown --code=lib     --to=code-haskell Tangle.md > src/Text/Pandoc/Tangle.hs

stack build
